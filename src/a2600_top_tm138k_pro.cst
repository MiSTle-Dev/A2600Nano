// DualShock controller on left PMOD slot
IO_LOC "joystick_clk" A17;
IO_PORT "joystick_clk" PULL_MODE=UP IO_TYPE=LVCMOS33;
IO_LOC "joystick_mosi" B20;
IO_PORT "joystick_mosi" PULL_MODE=UP IO_TYPE=LVCMOS33;
